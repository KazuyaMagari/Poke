{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/server.js"
      }
    ],
    "headers": [
      {
        "source": "/api/quiz/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://poke-v456.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ]
  }
  